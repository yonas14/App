<h1>Arsenal post</h1>


<div>
    <%= form_for(:session, url: login_path) do |f| %>

      <%= f.label :email %>
      <%= f.email_field :email%><br>

      <%= f.label :password %>
      <%= f.password_field :password %><br>

      <%= f.submit "Log in"%>
    <% end %>
    <p> </p>
    <p>New User: <%=link_to "Sign UP", new_user_path%></p>
